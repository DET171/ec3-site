{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "plugin:@typescript-eslint/recommended",
				"berry"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
			"react/react-in-jsx-scope": "off",
			"jsx-quotes": ["error", "prefer-single"],
			"next/no-html-link-for-pages": "off",
			"@typescript-eslint/no-explicit-any": "off"
    }
}
